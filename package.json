{
	"name": "@leodog896/bodyguard",
	"version": "1.3.2",
	"description": "Fetch API compatible streaming JSON and form data body parser and guard",
	"keywords": [
		"request",
		"fetch",
		"body-parsing"
	],
	"private": false,
	"type": "module",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "rimraf dist && tsc",
		"watch": "tsc -w",
		"test": "tsm node_modules/uvu/bin.js tests",
		"test:coverage": "c8 --include=src npm test",
		"test:report": "c8 report --reporter=text-lcov > coverage.lcov",
		"format": "dprint fmt"
	},
	"homepage": "https://github.com/LeoDog896/bodyguard",
	"repository": "github:LeoDog896/bodyguard",
	"contributors": [
		{
			"name": "miunau",
			"email": "miunau+npm@miunau.com",
			"url": "https://miunau.com"
		}
	],
	"files": [
		"dist",
		"LICENSE.md",
		"README.md"
	],
	"license": "MIT",
	"dependencies": {
		"@exact-realty/multipart-parser": "^1.0.10",
		"@streamparser/json": "^0.0.18"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"dprint": "^0.45.0",
		"rimraf": "^5.0.5",
		"tsm": "^2.3.0",
		"typescript": "^5.3.3",
		"uvu": "^0.5.6"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	}
}
